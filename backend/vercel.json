{
    "version": 2,
    "builds": [
      {
        "src": "code-quest/backend/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "code-quest/backend/api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/code-quest/backend/api/$1" }
    ]
  }
  